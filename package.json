{
  "name": "json-rules-engine-simplified",
  "version": "0.1.18",
  "description": "Simpl JSON rules engine",
  "private": false,
  "author": "mavarazy@gmail.com",
  "scripts": {
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.dist.js --mode production",
    "cs-check": "prettier -l $npm_package_prettierOptions '{src,test}/**/*.js'",
    "cs-format": "prettier $npm_package_prettierOptions '{src,test}/**/*.js' --write",
    "dist": "npm run build:lib && npm run build:dist",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "publish-to-npm": "npm run dist && npm publish && npm version patch",
    "tdd": "jest --watchAll",
    "test": "jest --coverage"
  },
  "prettierOptions": "--jsx-bracket-same-line --trailing-comma es5 --semi",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "npm run lint",
      "npm run cs-format",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "deep-equal": "2.0.5",
    "predicate": "2.0.0",
    "selectn": "1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "atob": "^2.1.2",
    "babel-loader": "^8.2.3",
    "codemirror": "^5.65.2",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "exit-hook": "^3.0.0",
    "express": "^4.17.3",
    "gh-pages": "^3.2.3",
    "has-flag": "^5.0.1",
    "html": "1.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.6",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RxNT/json-rules-engine-simplified.git"
  },
  "keywords": [
    "rules",
    "engine",
    "rules engine"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/RxNT/json-rules-engine-simplified#readme"
}
